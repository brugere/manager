{
  "name": "@ovh-ux/ufrontend-iframe",
  "version": "0.0.0",
  "private": true,
  "description": "OVHcloud iframe micro frontend framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/modules/ufrontend-iframe"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "dev:watch": "tsc --watch",
    "start": "lerna exec --stream --scope='@ovh-ux/ufrontend-iframe' --include-dependencies -- yarn run build",
    "prepare": "yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ufrontend-iframe' --include-dependencies -- yarn run dev",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ]
  }
}
